<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.study.spring.jdbc.IDao">
		<select id="getOrder" resultType="com.study.spring.jdbc.UserDTO">
			select o.id as ono1, c.name as cname1, c.job as cjob1
			from orders o
			join customers c
			on o.customer_id =c.id
		</select>
		
	</mapper>
    